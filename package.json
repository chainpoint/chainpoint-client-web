{
  "name": "chainpoint-client-web",
  "version": "0.0.4-16",
  "main": "./src/ProofApp.js",
  "author": "Cody Henshaw <cody@tierion.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chainpoint/chainpoint-client-web.git"
  },
  "description": "Embeddable Components for Creating and Verifying Proofs on the Chainpoint Network",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chainpoint-binary": "^4.0.1",
    "chainpoint-client": "^0.2.5",
    "chainpoint-parse": "^3.1.2",
    "classnames": "^2.2.5",
    "css-ns": "^1.1.4",
    "fecha": "^2.3.3",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "js-file-download": "^0.4.1",
    "js-sha256": "^0.9.0",
    "json-fmt": "^1.1.2",
    "lodash": "^4.17.10",
    "postcss-flexbugs-fixes": "3.2.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-animate-height": "^1.0.1",
    "react-container-dimensions": "^1.3.3",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.3.1",
    "react-dropzone": "^4.2.9",
    "react-hot-loader": "^4.6.3",
    "react-inlinesvg": "^0.8.1",
    "react-router-dom": "^4.2.2",
    "react-sizes": "^1.0.3",
    "react-textarea-autosize": "^6.1.0",
    "react-transition-group": "^2.3.0",
    "react-visibility-sensor": "^3.11.0",
    "validator": "^9.4.1",
    "velocity-animate": "^1.5.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "yarn run build:injectedDist && yarn run build:react:dist && yarn run build:examples",
    "build:injectedDist": "node scripts/build.js",
    "build:react:dist": "node scripts/build-react.js",
    "build:examples": "sed -i.bak -E \"s/bundle\\.(js|css)/bundle\\.\\1/\" ./examples/index.html && rm ./examples/index.html.bak",
    "deploy": "gh-pages -d build",
    "test": "echo \"No tests specified\"",
    "new-build:injectedDist": "webpack config/new-webpack.config.prod.js",
    "buildv2": "webpack --config new-webpack.config.prod.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "7.1.6",
    "babel-loader": "^8.0.5",
    "base64-inline-loader": "^1.1.1",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "2.0.8",
    "postcss-prefixer": "^2.1.0",
    "raw-loader": "^0.5.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^3.7.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.4"
  }
}
